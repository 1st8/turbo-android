{
  "settings": {
    "app_latest_version_code": 85,
    "custom_app_feature_enabled": true
  },
  "rules": [
    {
      "patterns": [
        ".*"
      ],
      "properties": {
        "context": "default",
        "uri": "turbolinks://fragment/web"
      }
    },
    {
      "patterns": [
        "^$",
        "/home"
      ],
      "properties": {
        "uri": "turbolinks://fragment/web/home",
        "presentation": "replace_all"
      }
    },
    {
      "patterns": [
        "/new$",
        "/edit$"
      ],
      "properties": {
        "context": "modal",
        "uri": "turbolinks://fragment/web/modal"
      }
    },
    {
      "patterns": [
        ".+\\.(?:bmp|gif|heic|jpg|jpeg|png|svg|webp)"
      ],
      "properties": {
        "uri": "turbolinks://fragment/image_viewer"
      }
    },
    {
      "patterns": [
        "/custom/refresh"
      ],
      "properties": {
        "presentation": "refresh"
      }
    },
    {
      "patterns": [
        "/custom/resume"
      ],
      "properties": {
        "presentation": "none"
      }
    }
  ]
}
