{
  "settings": {
    "screenshots_enabled": true
  },
  "rules": [
    {
      "patterns": [
        ".*"
      ],
      "properties": {
        "context": "default",
        "uri": "turbo://fragment/web",
        "pull_to_refresh_enabled": true
      }
    },
    {
      "patterns": [
        "^$",
        "^/$"
      ],
      "properties": {
        "uri": "turbo://fragment/web/home"
      }
    },
    {
      "patterns": [
        ".+\\.(?:bmp|gif|heic|jpg|jpeg|png|svg|webp)"
      ],
      "properties": {
        "uri": "turbo://fragment/image_viewer"
      }
    }
  ]
}
